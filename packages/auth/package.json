{
  "name": "@vatts/auth",
  "version": "1.2.0-alpha.31",
  "description": "Authentication package for Vatts.js framework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "author": "itsmuzin",
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.20.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.js"
    },
    "./vue": {
      "types": "./dist/vue/index.d.ts",
      "import": "./dist/vue/index.js",
      "require": "./dist/vue/index.js"
    }
  },
  "keywords": [
    "hightjs",
    "nytejs",
    "nyte",
    "vattsjs",
    "vatts",
    "auth",
    "authentication",
    "framework"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && copyfiles -u 1 \"src/**/*.vue\" dist",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/react": "^19.2.2",
    "copyfiles": "^2.4.1",
    "rimraf": "^5.0.0",
    "typescript": "^5.9.3",
    "vue": "^3.5.27"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "vatts": "workspace:^",
    "vue": "^3.5.27"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    }
  }
}
