{
  "name": "vatts",
  "version": "1.2.4",
  "description": "Vatts.js is a high-level framework for building web applications with ease and speed. It provides a robust set of tools and features to streamline development and enhance productivity.",
  "types": "dist/index.d.ts",
  "author": "mfraz",
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.28.1+sha512.7d7dbbca9e99447b7c3bf7a73286afaaf6be99251eb9498baefa7d406892f67b879adb3a1d7e687fc4ccc1a388c7175fbaae567a26ab44d1067b54fcb0d6a316",
  "keywords": [
    "vattsjs",
    "vatts",
    "framework",
    "web",
    "typescript",
    "react"
  ],
  "files": [
    "dist",
    "README.md",
    "dist/global/global.d.ts"
  ],
  "bin": {
    "vatts": "./dist/bin/vatts.js"
  },
  "scripts": {
    "build": "tsc && copyfiles -u 1 \"src/**/*.{vue,d.ts}\" dist"
  },
  "typesVersions": {
    "*": {
      "global": [
        "dist/global/global.d.ts"
      ],
      "console": [
        "dist/api/console.d.ts"
      ],
      "rpc": [
        "dist/rpc/annotations.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./console": {
      "types": "./dist/api/console.d.ts",
      "import": "./dist/api/console.js",
      "require": "./dist/api/console.js"
    },
    "./react": {
      "types": "./dist/react/client.d.ts",
      "import": "./dist/react/client.js",
      "require": "./dist/react/client.js"
    },
    "./vue": {
      "types": "./dist/vue/client.d.ts",
      "import": "./dist/vue/client.js",
      "require": "./dist/vue/client.js"
    },
    "./helpers": {
      "types": "./dist/helpers.d.ts",
      "import": "./dist/helpers.js",
      "require": "./dist/helpers.js"
    },
    "./global": {
      "types": "./dist/global/global.d.ts",
      "import": "./dist/global/global.d.ts"
    },
    "./eslint": {
      "types": "./dist/eslint/index.d.ts",
      "import": "./dist/eslint/index.js",
      "require": "./dist/eslint/index.js"
    },
    "./rpc": {
      "types": "./dist/rpc/annotations.d.ts",
      "import": "./dist/rpc/annotations.js",
      "require": "./dist/rpc/annotations.js"
    }
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "fastify": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "peerDependencies": {
    "express": "^4.0.0",
    "fastify": "^5.6.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "vue": "^3.5.27"
  },
  "dependencies": {
    "@rollup/plugin-alias": "^6.0.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.3",
    "@vue/server-renderer": "^3.5.27",
    "chokidar": "^3.6.0",
    "commander": "^14.0.2",
    "esbuild": "^0.27.2",
    "rollup": "^4.56.0",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-vue": "^6.0.0",
    "sharp": "^0.34.5",
    "ts-loader": "9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "ws": "^8.19.0"
  },
  "optionalDependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/formbody": "^8.0.2",
    "cookie-parser": "^1.4.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.25",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.18.1",
    "@vue/tsconfig": "^0.8.1",
    "copyfiles": "^2.4.1",
    "image-size": "^2.0.2",
    "vue": "^3.5.27",
    "vue-tsc": "^3.2.3"
  }
}
