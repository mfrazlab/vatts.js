{
  "name": "vatts",
  "version": "1.1.3",
  "description": "Vatts.js is a high-level framework for building web applications with ease and speed. It provides a robust set of tools and features to streamline development and enhance productivity.",
  "types": "dist/index.d.ts",
  "author": "itsmuzin",
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.20.0",
  "keywords": [
    "vattsjs",
    "vatts",
    "framework",
    "web",
    "typescript",
    "react"
  ],
  "files": [
    "dist",
    "README.md",
    "dist/global/global.d.ts"
  ],
  "bin": {
    "vatts": "./dist/bin/vatts.js"
  },
  "scripts": {
    "build": "tsc"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./console": {
      "types": "./dist/api/console.d.ts",
      "import": "./dist/api/console.js",
      "require": "./dist/api/console.js"
    },
    "./react": {
      "types": "./dist/client/client.d.ts",
      "import": "./dist/client/client.js",
      "require": "./dist/client/client.js"
    },
    "./helpers": {
      "types": "./dist/helpers.d.ts",
      "import": "./dist/helpers.js",
      "require": "./dist/helpers.js"
    },
    "./global": {
      "types": "./dist/global/global.d.ts"
    },
    "./eslint": {
      "types": "./dist/eslint/index.d.ts",
      "import": "./dist/eslint/index.js",
      "require": "./dist/eslint/index.js"
    },
    "./rpc": {
      "types": "./dist/rpc/annotations.d.ts",
      "import": "./dist/rpc/annotations.js",
      "require": "./dist/rpc/annotations.js"
    }
  },
  "peerDependencies": {
    "express": "^4.0.0",
    "fastify": "^5.6.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "fastify": {
      "optional": true
    }
  },
  "dependencies": {
    "@rollup/plugin-alias": "^6.0.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.3",
    "chokidar": "^3.6.0",
    "commander": "^14.0.2",
    "rollup": "^4.55.2",
    "rollup-plugin-esbuild": "^6.2.1",
    "sharp": "^0.34.5",
    "ts-loader": "9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "ws": "^8.19.0"
  },
  "optionalDependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/formbody": "^8.0.2",
    "cookie-parser": "^1.4.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.25",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.18.1",
    "image-size": "^2.0.2"
  }
}
